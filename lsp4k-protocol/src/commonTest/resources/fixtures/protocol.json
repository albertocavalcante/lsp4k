{
  "position": {
    "origin": {"line": 0, "character": 0},
    "typical": {"line": 10, "character": 5},
    "endOfLine": {"line": 100, "character": 0}
  },
  "range": {
    "singleLine": {
      "start": {"line": 10, "character": 0},
      "end": {"line": 10, "character": 20}
    },
    "multiLine": {
      "start": {"line": 5, "character": 0},
      "end": {"line": 15, "character": 10}
    },
    "empty": {
      "start": {"line": 0, "character": 0},
      "end": {"line": 0, "character": 0}
    }
  },
  "location": {
    "typical": {
      "uri": "file:///src/main.kt",
      "range": {
        "start": {"line": 10, "character": 0},
        "end": {"line": 10, "character": 20}
      }
    }
  },
  "textEdit": {
    "insert": {
      "range": {
        "start": {"line": 10, "character": 5},
        "end": {"line": 10, "character": 5}
      },
      "newText": "inserted text"
    },
    "replace": {
      "range": {
        "start": {"line": 10, "character": 0},
        "end": {"line": 10, "character": 10}
      },
      "newText": "replacement"
    },
    "delete": {
      "range": {
        "start": {"line": 5, "character": 0},
        "end": {"line": 6, "character": 0}
      },
      "newText": ""
    }
  },
  "diagnostic": {
    "minimal": {
      "range": {
        "start": {"line": 10, "character": 0},
        "end": {"line": 10, "character": 20}
      },
      "message": "Syntax error"
    },
    "withSeverity": {
      "range": {
        "start": {"line": 10, "character": 0},
        "end": {"line": 10, "character": 20}
      },
      "message": "Unused variable",
      "severity": 2
    },
    "full": {
      "range": {
        "start": {"line": 10, "character": 0},
        "end": {"line": 10, "character": 20}
      },
      "message": "Variable is deprecated",
      "severity": 2,
      "code": "DEPRECATION",
      "codeDescription": {"href": "https://kotlinlang.org/docs/deprecation.html"},
      "source": "kotlin-compiler",
      "tags": [2],
      "relatedInformation": [
        {
          "location": {
            "uri": "file:///other.kt",
            "range": {
              "start": {"line": 1, "character": 0},
              "end": {"line": 1, "character": 10}
            }
          },
          "message": "Declared here"
        }
      ]
    }
  },
  "completionItem": {
    "simple": {
      "label": "println"
    },
    "withKind": {
      "label": "myFunction",
      "kind": 3,
      "detail": "() -> Unit"
    },
    "full": {
      "label": "fullItem",
      "kind": 3,
      "detail": "(x: Int) -> String",
      "documentation": "This function does something useful",
      "deprecated": false,
      "preselect": true,
      "sortText": "0000fullItem",
      "filterText": "fullItem",
      "insertText": "fullItem($1)",
      "insertTextFormat": 2,
      "textEdit": {
        "range": {
          "start": {"line": 10, "character": 0},
          "end": {"line": 10, "character": 4}
        },
        "newText": "fullItem($1)"
      }
    }
  },
  "hover": {
    "simple": {
      "contents": "Simple hover text"
    },
    "markdown": {
      "contents": {
        "kind": "markdown",
        "value": "# Function\n\n```kotlin\nfun example(): Unit\n```"
      }
    },
    "withRange": {
      "contents": {"kind": "plaintext", "value": "Hover content"},
      "range": {
        "start": {"line": 5, "character": 0},
        "end": {"line": 5, "character": 10}
      }
    }
  },
  "publishDiagnostics": {
    "empty": {
      "uri": "file:///clean.kt",
      "diagnostics": []
    },
    "withVersion": {
      "uri": "file:///test.kt",
      "version": 5,
      "diagnostics": [
        {
          "range": {
            "start": {"line": 0, "character": 0},
            "end": {"line": 0, "character": 10}
          },
          "message": "Error message",
          "severity": 1
        }
      ]
    }
  }
}
